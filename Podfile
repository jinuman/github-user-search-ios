platform :ios, '12.0'

target 'MyGithubUserSearch' do
  use_frameworks!

  # Pods for MyGithubUserSearch

  # Architecture
  pod 'ReactorKit'

  # Rx
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RxDataSources'
  pod 'RxOptional'

  # Others
  pod 'Then'
  pod 'ReusableKit'
  pod 'Kingfisher'
  pod 'SwiftLint'

  target 'MyGithubUserSearchTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Quick'
    pod 'Nimble'
    pod 'RxTest'
  end
end
